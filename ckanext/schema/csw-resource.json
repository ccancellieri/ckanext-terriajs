{
  "type": "object",
  "required": [
    "id",
    "type",
    "name",
    "url",
    "containerId",
    "itemProperties"
  ],
  "oneOf":[
    {
      "$ref": "base.json"
    }
  ],
  "properties":{
    "type": {
      "type": "string",
      "enum": [
        "csw-resource"
      ]
    },
    "id": {
      "$ref":"id.json"
    },
    "name": {
      "title":"name",
      "type":"string"
    },
    "description": {
      "$ref":"description.json"
    },
    "url": {
      "type": "string"
    },
    "containerId": {
      "type": "string",
      "description": "metadata target UUID"
    },
    "itemProperties":{
      "type":"object",
      "required": [
        "layers"
      ],
      "featureInfoTemplate": {
        "type": "string"
      },
      "layers": {
        "type": "string"
      },
      "customProperties": {
        "type":"object",

        "dimensionLabelsConfig": {
          "type":"object",

          "dimensionsJsonStatUrl": {
            "type":"string"
          },
          "showDimensionsOnPanel": {
            "type":"string",
            "enum":["false","true"]
          }
        }
      },
      "ignoreUnknownTileErrors": {
        "type":"string",
        "enum":["false","true"]
      }
    }
  }
}